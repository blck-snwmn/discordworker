{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "discordworker",
	"main": "src/worker.ts",
	"compatibility_date": "2024-12-24",
	"compatibility_flags": ["nodejs_compat"],
	"workers_dev": false,
	"preview_urls": false,
	"observability": {
		"enabled": true
	},
	"queues": {
		"consumers": [
			{
				"queue": "discordqueue"
			}
		],
		"producers": [
			{
				"queue": "discordqueue",
				"binding": "DQUEUE"
			}
		]
	}
}
